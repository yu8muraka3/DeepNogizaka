  <div class="sidrNavi-overlay"></div>
  <script src="/ent/m/js/lib/sidr-package-1.2.1/jquery.sidr.min.js"></script>
  <script> window.dataLayer = []; </script>
  <script>
  //ドロワーメニューの中身を初期化
  getMenuJson(function(err, data){
    var matchedMenu = null;
    data.menu.forEach(function(m){
      if(!matchedMenu) {
        var re = new RegExp(m.route);
        var pathSearch = location.pathname + location.search;
        if(re.test(pathSearch)) {
          var g = [1,2,3,4,5].map(function(i){ return ["$"+i, RegExp["$"+i]]; });
          if(m.items) {
            m.items = m.items.map(function(v){
              g.forEach(function(kv){
                v.url = v.url.replace(kv[0], kv[1]);
              });
              return v;
            });
          }
          matchedMenu = m;
        }
      }
    });
    if(matchedMenu && matchedMenu.items && 0 < matchedMenu.items.length) {
      var ul = $(".sidrNavi ul").empty();
      matchedMenu.items.forEach(function(item){
        ul.append($("<li>").append($("<a>").attr({"data-href":item.url}).text(item.title)));
      });
      enableDrawerMenu();
    } else {
      $("#menu")
        .addClass(matchedMenu.iconClass) //エンタメアイコン表示
        .on("click touchstart mousedown", function(){ //TOPへジャンプ
          location.href = location.pathname.replace(/\/(m|member)\/.*/, '') + "/m/";
        });
    }
    //ドロワーメニューの動作を宣言
    function enableDrawerMenu() {
      $('#menu:not([class~="menuEnt"])').sidr({
        name: 'sidrNavi',
        source: '#sidrNavi',
        displace: false,
        speed: 100,
        onOpen: function(){
          $(".sidrNavi-overlay").show();
          //ちょっと送れてリンクを有効にする（メニューを出すためのタップ操作が、開いた後に指の下にいるリンクに影響する端末が要るため…）
          setTimeout(function(){
            $(".sidrNavi a[data-href]").each(function(){
              var a=$(this);
              a.attr({href:a.data().href});
            });
          }, 500);
        },
        onClose: function(){
          $(".sidrNavi-overlay").hide();
          //次回オープン時にリンクが無効になってるようにする
          $(".sidrNavi a[data-href]").each(function(){
            $(this).attr({href:null});
          })
        }
      });
      $(".sidrNavi-overlay, .sidrNavi").on('click', function(event){
        if(!$(event.target).closest('#sidrNavi a').length) {
          $.sidr('close', 'sidrNavi');
        }
      });
    }
  });
  //menu.jsonを取ってくる関数（スクロールメニューと共有するため後でsp.jsに移動するかも）
  function getMenuJson(cb) {
    if(getMenuJson._cache) {
      return getMenuJson._cache;
    }
    $.getJSON("/ent/m/data/menu.json", function(data, status, xhr){
      if(status == "success") {
        getMenuJson._cache = data;
        cb(null, data);
      } else {
        cb(status);
      }
    });
  }
  </script>
